    "a143eef7-1761-481d-b8d1-406fad0ca015": {
      "type": "sec-top-secondary-service",
      "settings": {
        "section-name": "workshop",
        "side-image": "shopify:\/\/shop_images\/img_top-workshop.jpg",
        "title-en": "WORKSHOP",
        "title": "ワークショップ",
        "description": "<p>PRINT+PLANTでは活版印刷やリソグラフを中心としたアナログ印刷の研究を行っています。他にはない、より突っ込んだ印刷体験をワークショップや教室として定期開催しております。<br>\n是非お気軽にご参加ください。<\/p>",
        "link": "shopify:\/\/blogs\/workshop",
        "linktext-en": "WORKSHOP",
        "linktext-ja": "ワークショップ"
      }
    },
